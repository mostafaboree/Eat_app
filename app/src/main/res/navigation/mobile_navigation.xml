<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.mostafabor3e.eat_app.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <activity
        android:id="@+id/nav_gallery"
        android:name="com.mostafabor3e.eat_app.ui.Cart"
        android:label="activity_cart"
        tools:layout="@layout/activity_cart" />
    <activity
        android:id="@+id/order"
        android:name="com.mostafabor3e.eat_app.ui.Order"
        android:label="activity_order"
        tools:layout="@layout/activity_order" />
    <activity
        android:id="@+id/logout"
        android:name="com.mostafabor3e.eat_app.ui.StartActivity"
        android:label="activity_start"
        tools:layout="@layout/activity_start" />
</navigation>